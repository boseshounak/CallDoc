<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="shounak.shounakbosejiit.com.calldoc.BookAsk">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="281dp"
        android:layout_height="44dp"
        android:layout_marginBottom="451dp"
        android:layout_marginLeft="16dp"
        android:text="Doctor's Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/e_docname"
        android:layout_width="349dp"
        android:layout_height="54dp"
        android:layout_marginBottom="397dp"
        android:layout_marginLeft="16dp"
        android:elevation="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="281dp"
        android:layout_height="44dp"
        android:layout_marginBottom="324dp"
        android:layout_marginLeft="31dp"
        android:text="Doctor's Category"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/e_doctype"
        android:layout_width="349dp"
        android:layout_height="54dp"
        android:layout_marginBottom="270dp"
        android:layout_marginRight="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="281dp"
        android:layout_height="44dp"
        android:layout_marginBottom="178dp"
        android:layout_marginLeft="31dp"
        android:text="Time-Slot"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/e_timeslot"
        android:layout_width="349dp"
        android:layout_height="54dp"
        android:layout_marginBottom="124dp"
        android:layout_marginRight="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/bpro"
        android:layout_width="104dp"
        android:layout_height="44dp"
        android:text="PROCEED"
        android:background="@drawable/large_btn_blue"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="52dp"
        android:layout_marginLeft="55dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/bcan"
        android:layout_width="104dp"
        android:layout_height="44dp"
        android:layout_marginBottom="52dp"
        android:layout_marginRight="43dp"
        android:background="@drawable/large_btn_red"
        android:text="CANCEL"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
